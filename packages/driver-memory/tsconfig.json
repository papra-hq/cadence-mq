{
  "compilerOptions": {
    "target": "es2024",
    "lib": ["es2024"],
    "moduleDetection": "force",
    "module": "preserve",
    "resolveJsonModule": true,
    "allowJs": true,
    "strict": true,
    "noImplicitOverride": true,
    "noUncheckedIndexedAccess": true,
    "declaration": true,
    "sourceMap": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "verbatimModuleSyntax": true,
    "skipLibCheck": true
  }
}
