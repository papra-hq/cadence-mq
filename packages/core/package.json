  "dependencies": {
    "@corentinth/chisels": "^1.3.1",
    "@paralleldrive/cuid2": "^2.2.2",
    "@standard-schema/spec": "^1.0.0",
    "cron-parser": "^5.3.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "catalog:",
    "@vitest/coverage-v8": "catalog:",
    "eslint": "catalog:",
